<%= form_for @user do |f| %>
<%= f.label :username  %><br>
<%= f.text_field :username %><br>
<%= f.label :name  %><br>
<%= f.text_field :name %><br>
<%= f.label :age  %><br>
<%= f.text_field :age %><br>
<%= f.label :password  %><br>
<%= f.text_field :password %><br>
<%= f.label :location  %><br>
<%= f.text_field :location %><br>
<%= f.submit nil, class: 'f_submit'  %>
<% end %>